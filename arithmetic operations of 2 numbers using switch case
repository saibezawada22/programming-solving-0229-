#include<stdio.h>
int main()
{
    int ch;
    float a,b,sum,sub,div,mul;
    printf("enter a,b");
    scanf("%f%f",&a,&b);
    printf("enter choice");
    scanf("%d",&ch);
    switch(ch)
    {
        case 1:
        sum=a+b;
        printf("sum is %.2f",sum);
        break;
        case 2:
        sub=a-b;
        printf("sub is %.2f",sub);
        break;
        case 3:
        mul=a*b;
        printf("mul is %.2f",mul);
        break;
        case 4:
        div=a/b;
        printf("div is %.2f",div);
        break;
        default:
        printf("enter crt choice");
        break;
    }
}
