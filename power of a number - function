#include<stdio.h>
void getpower(int b,int e);
void main()
{
    int b,e;
    printf("enter \nbase\nexponent");
    scanf("%d%d",&b,&e);
    getpower(b,e);
}
void getpower(int b, int e)
{
    int power=1;
    while(e>0)
    {
        power=power*b;
        e--;
    }
    printf("power is %d",power);
}
